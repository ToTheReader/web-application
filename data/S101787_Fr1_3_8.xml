  <text>
     <body>
     <div xml:lang="lat" type="poem">
   <head>Thomas Nortonus Sharpenhauius Lectori.</head>
   <lg>
      <l>
         <hi>Quando sic summo pariter parenti</hi>
      </l>
      <l>
         <hi>Constat &amp; rerum domino placere,</hi>
      </l>
      <l>
         <hi>Cuius in cunctis manet una semper</hi>
      </l>
      <l>
         <hi>firma uoluntas:</hi>
      </l>
   </lg>
   <lg>
      <l>
         <hi>Vt uel exipsis etiant malorum</hi>
      </l>
      <l>
         <hi>Improbe factis sua proferatur</hi>
      </l>
      <l>
         <hi>Laus, quod ruentus moderetur at{que}</hi>
      </l>
      <l>
         <hi>tempora solus:</hi>
      </l>
   </lg>
   <lg>
      <l>
         <hi>Commodum lector simul ó beate</hi>
      </l>
      <l>
         <hi>In tuum cedant, animosa quisquis</hi>
      </l>
      <l>
         <hi>In tui causa ducis exhibebis</hi>
      </l>
      <l>
         <hi>pectora Christi:</hi>
      </l>
   </lg>
   <pb facs="8"/>
   <lg>
      <l>
         <hi>In tuum proles igitur Satanae</hi>
      </l>
      <l>
         <hi>Quicquid hic Christum uomuit ueneni,</hi>
      </l>
      <l>
         <hi>Robur integro, medicamen aegro</hi>
      </l>
      <l>
         <hi>en tibi factum.</hi>
      </l>
   </lg>
   <lg>
      <l>
         <hi>Is quod errauit, moneat supernae</hi>
      </l>
      <l>
         <hi>Candido grates animo ferendas</hi>
      </l>
      <l>
         <hi>Gratiae, quate sibi chariorem</hi>
      </l>
      <l>
         <hi>praetulit illi.</hi>
      </l>
   </lg>
   <lg>
      <l>
         <hi>Insuper quae sint, doceat cauenda.</hi>
      </l>
      <l>
         <hi>Fausta Turnero medico precare</hi>
      </l>
      <l>
         <hi>Tam pio: mentem quoque saniorem</hi>
      </l>
      <l>
         <hi>haeresiarchae.</hi>
      </l>
   </lg>
</div>
